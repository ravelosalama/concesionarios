      object daSQL3: TdaSQL
        GuidCollationType = gcMSSQLServer
        DatabaseName = 'SrvConnection'
        DatabaseType = dtMSSQLServer
        DataPipelineName = 'Items_factura'
        LinkColor = clGreen
        LinkType = ltMagicSQL
        MaxSQLFieldAliasLength = 25
        SQLText.Strings = (
          'SELECT SAITEMFAC.CantMayor, SAITEMFAC.CantidadC, '
          '       SAITEMFAC.Cantidad, SAITEMFAC.CodUbic, '
          '       SAITEMFAC.CodItem, SAITEMFAC.CodMeca, '
          '       SAITEMFAC.CodVend, SAITEMFAC.Costo, '
          '       SAITEMFAC.Descrip1, SAITEMFAC.Descrip10, '
          '       SAITEMFAC.Descrip2, SAITEMFAC.Descrip3, '
          '       SAITEMFAC.Descrip4, SAITEMFAC.Descrip5, '
          '       SAITEMFAC.Descrip6, SAITEMFAC.Descrip7, '
          '       SAITEMFAC.Descrip8, SAITEMFAC.Descrip9, '
          '       SAITEMFAC.Descto, SAITEMFAC.FechaE, '
          '       SAITEMFAC.EsExento, SAITEMFAC.DEsLote, '
          '       SAITEMFAC.DEsSeri, SAITEMFAC.EsServ, '
          '       SAITEMFAC.EsUnid, SAITEMFAC.Status, '
          '       SAITEMFAC.ExistAnt, SAITEMFAC.ExistAntU, '
          '       SAITEMFAC.FechaL, SAITEMFAC.NroLote, '
          '       SAITEMFAC.MtoTax, SAITEMFAC.NroLinea, '
          '       SAITEMFAC.NroLineaC, SAITEMFAC.NroUnicoL, '
          '       SAITEMFAC.NumeroD, SAITEMFAC.NumeroE, '
          '       SAITEMFAC.Precio, SAITEMFAC.PriceO, '
          '       SAITEMFAC.Refere, SAITEMFAC.Signo, '
          '       SAITEMFAC.CodSucu, SAITEMFAC.Tara, '
          '       SAITEMFAC.TipoFac, SAITEMFAC.TotalItem, '
          '       SAITEMFAC.CantidadU, SAITEMFAC.EsPesa, '
          '       SAITEMFAC.UsaServ, SAITEMFAC.FechaV, '
          '       VW_SAFACT.TotalPrd, VW_SAFACT.TotalSrv, '
          '       VW_SAFACT.TGravable, VW_SAFACT.TExento, '
          '       VW_SAFACT.MtoTax MtoTax_2, '
          '       VW_SAFACT.MtoTotal, '
          '       VW_SAFACT.FechaE FechaE_2, '
          '       VW_SAFACT.CodOper, '
          '       VW_SAFACT.CodVend CodVend_2, '
          '       VW_SAFACT.CodClie, SAMECA.Descrip, '
          '       VW_SAFACT.Signo Signo_2, '
          
            '       SAITEMFAC.TOTALITEM*VW_SAFACT.SIGNO SAITEMFAC_TOTALITEM_V' +
            'W_SA, '
          '       SAITEMFAC.COSTO*VW_SAFACT.SIGNO SAITEMFAC_COSTO_VW_SAFACT'
          'FROM SAITEMFAC SAITEMFAC'
          '      LEFT OUTER JOIN VW_SAFACT VW_SAFACT ON '
          '     (VW_SAFACT.TipoFac = SAITEMFAC.TipoFac)'
          '      AND (VW_SAFACT.NumeroD = SAITEMFAC.NumeroD)'
          '      LEFT OUTER JOIN SAMECA SAMECA ON '
          '     (SAMECA.CodMeca = SAITEMFAC.CodMeca)'
          'WHERE '
          
            '      ( VW_SAFACT.FechaE BETWEEN CONVERT(DATETIME,'#39'2016-09-01'#39',1' +
            '20) AND CONVERT(DATETIME,'#39'2016-10-01'#39',120) )'
          '       AND ( VW_SAFACT.TipoFac IN ('#39'A'#39','#39'B'#39') )'
          '       AND ( VW_SAFACT.CodOper = '#39'01-301'#39' )'
          '       AND ( SAITEMFAC.EsServ = 1 )'
          'ORDER BY SAITEMFAC.CodMeca, SAITEMFAC.FechaE')
